               RowBox[{"href_", ",", "\"\<Text\>\"", ",", "___"}], "]"}]}], 
             "}"}], ",", "Open"}], "]"}], "]"}], "\[RuleDelayed]", 
         RowBox[{"<|", 
          RowBox[{
           RowBox[{"\"\<caption\>\"", "\[Rule]", "caption"}], ",", 
           RowBox[{"\"\<text\>\"", "\[Rule]", "text"}], ",", 
           RowBox[{"\"\<href\>\"", "\[Rule]", "href"}]}], "|>"}]}], ",", 
        "\[Infinity]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Scan", "[", 
      RowBox[{

Cell[BoxData[
 RowBox[{
  RowBox[{"build", "[", "file_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "basename", ",", "basedir", ",", "notebook", ",", "webpage", ",", 
      "stream", ",", "text", ",", "data"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"basename", "=", 
      RowBox[{"FileBaseName", "[", "file", "]"}]}], ";", 
     "\[IndentingNewLine]", 

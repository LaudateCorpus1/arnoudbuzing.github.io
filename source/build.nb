     "\[IndentingNewLine]", 
     RowBox[{"notebook", "=", 
      RowBox[{
       RowBox[{"Import", "[", 
        RowBox[{"file", ",", "\"\<Notebook\>\""}], "]"}], "/.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"HoldPattern", "[", "_Rule", "]"}], "\[RuleDelayed]", 
         RowBox[{"Sequence", "[", "]"}]}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"webpage", "=", 
      RowBox[{"FileNameJoin", "[", 
       RowBox[{"{", 
        RowBox[{"basedir", ",", 
